
RewriteEngine on
# Deny folder listing
Options -Indexes

# X-Frame-Options, only allow browser access from sameorigin
<IfModule mod_headers.c>
	Header set X-Frame-Options "SAMEORIGIN"
   Header unset X-Powered-By
   Header set Access-Control-Allow-Origin "*"
</IfModule>

# Disable server signature
ServerSignature Off

RewriteOptions inherit

# RewriteCond %{REQUEST_URI} "/auth/"
# RewriteCond %{REQUEST_URI} "/mmt-admin/" [OR]
# RewriteCond %{REQUEST_URI} "/studentmobileapp.com/" [OR]
# RewriteRule (.*) $1 [L]

RewriteRule ^my\-mobile\-tutor\-kayttoehdot\/$ "https\:\/\/studentmobileapp\.com\/utu\/my\-mobile\-tutor\-kayttoehdot\.pdf" [R=302,L]
RewriteRule ^my\-mobile\-tutor\-tietosuojaseloste$ "https\:\/\/studentmobileapp\.com\/utu\/my\-mobile\-tutor\-tietosuojaseloste\.pdf" [R=302,L]
RewriteRule ^my\-mobile\-tutor\-terms\-of\-service\/$ "https\:\/\/studentmobileapp\.com\/utu\/my\-mobile\-tutor\-terms\-of\-service\.pdf" [R=302,L]
RewriteRule ^my\-mobile\-tutor\-privacy\-policy$ "https\:\/\/studentmobileapp\.com\/utu\/my\-mobile\-tutor\-privacy\-policy\.pdf" [R=302,L]

